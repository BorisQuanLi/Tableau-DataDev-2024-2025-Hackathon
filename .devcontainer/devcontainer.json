{
    "name": "FastAPI Hackathon Project",
    "context": "..",
    "dockerFile": "Dockerfile",
    "workspaceFolder": "/workspace",
    "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker"
    ],
    "settings": {
        "python.pythonPath": "/usr/local/bin/python"
    },
    "postCreateCommand": "pip install -r backend/requirements.txt",
    "remoteUser": "root"
}